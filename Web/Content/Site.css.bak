/* uvjeti pretraživanja */
.search_body
{
	margin: 0px auto 0 auto;
	width: 100%;
	position: relative;
}

/* pretražnik menu */
.search_list
{
	width: 100%;
}

.search_list ul
{
	width: 100% !important;
	float: none !important;
	height: 25px;
	padding: 0;
    background-color: #4F97E3;
	box-shadow: inset 0 0 5px rgba(0,0,0,0.3);
    
    border-collapse: collapse;
}

.search_list ul li
{
	height: 100%;
	display: inline-block;
	width: 20%;
	height: 100%;
	text-align: center;
	padding: 0;
	margin: 0;
	position: relative;
}

.search_list.items8 ul li{
	width: 12.5%;
}

.search_list.items7 ul li{
	width: 14.28%;
}

.search_list li a
{
    font-size: 15px;
    line-height: 25px;
	text-decoration: none;
	position: absolute;
	top: 0;
	bottom: 0;
	left: 0;
	right: 0;
	color: #fff;
}

/* odabrana stavka menija */
.search_list li a.selected
{
	background: #1E6DC2 linear-gradient(to bottom, #1E6DC2, #1C64B0);
    /*box-shadow: 0 0 2px rgba(0,0,0,0.5);*/
    /*height: 30px;
    line-height: 30px;
    margin-top: -2px;
	z-index: 1;*/
}

.search_list li a.highlighted
{
    background-color: #1E6DC2;
}

/* dodatni uvjeti pretrage */
.search_extra_toggle
{
	display: block;
	height: 20px;
	width: 100%;
	position: relative;
}

.search_extra_toggle input[type=checkbox]
{
	display: none;
}

.search_extra_toggle input[type=checkbox] + label
{
	cursor: pointer;
	display: inline-block;
	padding: 0;
	margin: 0;
	min-width:  initial;
	width: 25px;
	height: 20px;
	line-height: 20px;
	text-align: center;
	position: absolute;
	right: 0;
	z-index: 0;
	background: #4F97E3 url(../Images/plus_white.png) no-repeat center center;
	opacity: 0.3;
	transition: opacity 0.3s;
}

.search_extra_toggle input[type=checkbox] + label:hover
{
	opacity: 1;
}

.search_extra_toggle input[type=checkbox]:checked + label
{
	background-color: rgba(0,0,0,0.5);
	color: #fff;
	background: #4F97E3 url(../Images/minus_white.png) no-repeat center center;
}

.search_extra
{
	/*border: 1px solid #AAA;*/
	/*background: rgba(79,151,227,0.04);*/
    /*border-top: 1px solid rgba(79,151,227,0.05);*/
	padding: 0;
    height: 28px;
    position: relative;
	border-top: 1px solid #e0e0e0;
	box-shadow: inset 0 1px 0 #fff;
}

.search_extra:last-child
{
    border-bottom: 1px solid #ddd;
}

.search_extra_label
{
    display: inline-block;
