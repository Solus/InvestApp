.blog_container{
	width: 700px;
	margin: 0 auto;
}

.blog_item{
	position: relative;
	margin-bottom: 20px;
	padding: 10px 15px;
	background: #f5f5f5;
	border-radius: 1px;
	box-shadow: 0 1px 2px rgba(0,0,0,.1);
}

.blog_item .form_item{
	margin: 10px 0;
}
.blog_item .btn_navigation_group{
	background: none;
	padding: 0;
	padding-top: 5px;
}

.blog_title{
	font-size: 1.6em;
	padding-bottom: 2px;
	border-bottom: 1px solid #bbb;
	color: #333;
}

.blog_info{
	padding: 1px 5px;
	text-align: right;
}

.blog_time{
	font-size: 0.8em;
	color: #888;
}

.blog_text{
	margin-top: 5px;
	text-align: justify;
}

.blog_button{
	display: block;
	width: 20px;
	height: 20px;
	position: absolute;
	top: 5px;
}

.blog_button.edit{
	right: 30px;
	background: url(../Images/edit.png) no-repeat center center;
}

.blog_button.delete{
	right: 5px;
	background: url(../Images/trash.png) no-repeat center center;
}

input.btn_blog_novi{
	margin: 0;
	margin-bottom: 15px;
