    border-radius: 5px;
    background-color: rgba(0,0,0,0.25);
    margin: 5px;
    padding-left: 10px;
    box-shadow: inset 0 1px 3px rgba(0, 0, 0, 0.3);
    color: #eee;
    outline: 0;
    transition: all 0.5s;
}

.user_box input[type=text]:focus,
.user_box input[type=password]:focus,
.user_box input[type=email]:focus
{
    background-color: rgba(255,255,255,0.1);
}

.user_box .btn_group
{
	text-align: right;
	margin-top: 5px;
}

.user_box input[type=submit]
{
    background-color: rgba(255,255,255,0.05);
    border: 1px solid rgba(0,0,0,0.2);
    border-right: none;
    border-bottom: none;
    border-radius: 5px 0 5px 0;
    color: #eee;
    margin: 0;
    padding: 10px 15px;
    outline: 0;
}

.user_box input[type=submit]:hover
{
    background-color: rgba(0,0,0,0.05);
}

.user_box input[type=submit]:active
{
    background-color: rgba(0,0,0,0.2);
}

.user_box a
{
	color: #eee;
}

.user_box .register_link
{
	position: absolute;
	left: 10px;
	bottom: 10px;
    text-decoration: none;
    transition: font-size 0.2s;
}

.user_box a.register_link:hover
{
    font-size: 18px;
}

.user_box .login_extra
{
	text-align: center;
	position: absolute;
	top: 101%;
	width: 500px;
}

.user_box .login_extra a
{
	color: #555;
}

